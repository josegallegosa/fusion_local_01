<odoo>
    <data>
        <record model='ir.ui.view' id='product_template_form_view_inherit'>
			<field name='name'>product.template.form.view.inherit</field>
			<field name='model'>product.template</field>
			<field name='inherit_id' ref="product.product_template_form_view"/>
			<field name='arch' type='xml'>
				<field name="company_id" position="before">
					<field name="product_brand"/>
					<field name="product_model"/>
					<field name="product_color"/>
				</field>
				<xpath expr="//page[@name='inventory']/group[@name='inventory']/group[@name='group_lots_and_weight']/div[@name='volume']" position="after">
                    <field name="product_length"/>
                    <field name="product_width"/>
                    <field name="product_high"/>
                </xpath>
			</field>
		</record>
    </data>
</odoo>