<odoo>
    <data>
        <record id="view_oms_catalog_sale_order_form" model="ir.ui.view">
            <field name="name">oms.catalog.view.sale.view_order_form.form</field>
            <field name="model">sale.order</field>
            <field name="inherit_id" ref="sale.view_order_form"/>
            <field name="priority">1000</field>
            <field name="arch" type="xml">
            
                 <xpath expr="//page[last()]" position="after">
                    <page string="Mercantil" name="merchant_account">
                        <group>
                            <field name="merchant_account"/>
                        </group>  
                        <group>
                            <field name="fsm_order_ids"/>
                        </group>                  
                    </page>
                </xpath>

                <field name="partner_id" position="attributes">
                    <attribute name="domain">[('type','in',['contact']),('fsm_location','=',False)]</attribute>
                </field>
                <field name="partner_invoice_id" position="attributes">
                    <attribute name="domain">[('type','=','invoice')]</attribute>                
                </field>
                <field name="partner_shipping_id" position="attributes">
                    <attribute name="domain">[('type','=','delivery')]</attribute>                
                </field>

            </field>
        </record>

    </data>
</odoo>