<odoo>

        <template id="portal_merchant_orders_fsm" name="Mis Ordenes" inherit_id="portal.portal_my_home" customize_show="True" priority="30">

            <xpath expr="//div[hasclass('o_portal_docs')]" position="inside">

                <t t-call="portal.portal_docs_entry">
                  <t t-set="title">Ordenes de Trabajo</t>                  
                  <t t-set="count" t-value="request.env['sale.order'].sudo().get_fsm_orders_count()"/>
                  <t t-set="url" t-value="'/my/fsm_orders'"/>
                  <t t-set="placeholder_count" t-value="'count'"/>
                </t>

            </xpath>

        </template>

</odoo>